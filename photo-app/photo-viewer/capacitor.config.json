{
  "appId": "com.photoviewer.app",
  "appName": "写真图库",
  "webDir": "dist",
  "server": {
    "androidScheme": "http",
    "hostname": "localhost",
    "allowNavigation": [
      "*"
    ],
    "cleartext": true
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true
  },
  "plugins": {
    "CapacitorSQLite": {
      "iosDatabaseLocation": "Library/CapacitorDatabase",
      "androidIsEncryption": false
    }
  }
}
